	<queryString>
		<![CDATA[select  gk.name as "Kps", mp.value as "Kode Bisnis",ci.documentno "No Sertifikat",extract(MONTH from dateinvoiced) as "Bulan", ci.dateinvoiced as "Tanggal",bp.name as "Nama Makful lahu",ffa.accountno as "No Rekening Pelunasan",gid.debitur as debitur,cv.combination as "Kode Akuntansi",
coalesce(gid.kafalah,0) as "Kafalah",(ci.outstandingamt - coalesce(gid.administrasi,0)) as "Ujrah",coalesce(gid.administrasi,0) as "admin",ci.outstandingamt as piutang,
($P{TanggalLaporan} - ci.dateinvoiced::timestamp::date) AS intervalnya,

case when ($P{TanggalLaporan} - ci.dateinvoiced::timestamp::date) <=30 then TO_NUMBER(((ci.outstandingamt*0)/100)) else NULL END AS "0%",

case when ($P{TanggalLaporan}- ci.dateinvoiced::timestamp::date) >= 31 AND
($P{TanggalLaporan} - ci.dateinvoiced::timestamp::date) <= 60  then TO_NUMBER(((ci.outstandingamt*5)/100)) else NULL END AS "5%",

case when ($P{TanggalLaporan} - ci.dateinvoiced::timestamp::date) >= 61 AND
($P{TanggalLaporan} - ci.dateinvoiced::timestamp::date) <= 90  then TO_NUMBER(((ci.outstandingamt*7.5)/100)) else NULL END AS "7.5%",


case when ($P{TanggalLaporan} - ci.dateinvoiced::timestamp::date) >= 91 AND
($P{TanggalLaporan} - ci.dateinvoiced::timestamp::date) <= 180  then TO_NUMBER(((ci.outstandingamt*10)/100)) else NULL END AS "10%",

case when ($P{TanggalLaporan}- ci.dateinvoiced::timestamp::date) >= 181 then TO_NUMBER(((ci.outstandingamt*15)/100)) else NULL END AS "15%",
ci.description as keterangan


from c_invoice ci
         left join gai_kantor gk on (ci.em_gai_kantor_id = gk.gai_kantor_id)
	 left join c_bpartner bp on (bp.c_bpartner_id=ci.c_bpartner_id)
	 left join c_invoiceline cil on (cil.c_invoice_id = ci.c_invoice_id)
	 left join m_product mp on (mp.m_product_id = cil.m_product_id )
	 left join M_Product_Category_Acct mpc on (mpc.m_product_category_id =mp.m_product_category_id)
	 left join c_validcombination cv on (cv.c_validcombination_id = mpc.p_revenue_acct)
	 LEFT join gai_invoice_detail gid on (gid.c_invoice_id = ci.c_invoice_id)
	 left join fin_financial_account ffa on (ffa.fin_financial_account_id = gid.fin_financial_account_id)
where ci.issotrx = 'Y'
and mp.value not like 'ADM%']]>
	</queryString>
